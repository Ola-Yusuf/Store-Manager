{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env":{
        "node": true,
        "browser": true,
        "mocha": true
    },
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "semi": 0,
      "max-len": 0,
      "no-multiple-empty-lines": 0,
      "object-shorthand": 0,
      "padded-blocks": 0
    }
  }